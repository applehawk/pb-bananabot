{
  "name": "bananabot-grammy",
  "version": "1.0.0",
  "description": "Telegram VPN Bot with NestJS and GrammyJS",
  "author": "VladMac",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "build:grammy": "nest build --entryFile main-grammy",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start --entryFile main-grammy",
    "start:dev": "nest start --watch --entryFile main-grammy",
    "start:debug": "nest start --debug --watch --entryFile main-grammy",
    "start:prod": "node dist/src/main-grammy",
    "start:migrate:prod": "npm run prisma:migrate:deploy && npm run start:prod",
    "webhook:set": "ts-node scripts/set-webhook.ts",
    "lint": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@google/generative-ai": "^0.21.0",
    "@grammyjs/conversations": "^2.1.0",
    "@grammyjs/hydrate": "^1.6.0",
    "@grammyjs/menu": "^1.2.2",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/storage-redis": "^2.5.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.7",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.7",
    "@nestjs/platform-express": "^11.1.7",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.19.0",
    "axios": "^1.6.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "grammy": "^1.38.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "luxon": "^3.7.2",
    "nanoid": "^5.0.4",
    "nestjs-pino": "^4.4.1",
    "pino": "^9.14.0",
    "pino-pretty": "^10.3.1",
    "prisma": "^6.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.1",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "yoomoney-sdk": "^2.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.6",
    "@nestjs/testing": "^11.1.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.46.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/../libs/$1/src",
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "npmAudit": {
    "ignore": [
      "GHSA-mh29-5h37-fv8m"
    ]
  }
}
